{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "tbblocks/slider-item",
	"version": "0.1.5",
	"title": "Slider Item",
	"category": "theme",
    "keywords": ["slider", "slide", "item"],
	"description": "A gutenberg block to use for Slider Item.",
	"supports": {
		"html": false,
		"anchor": true
	},
    "attributes": {
        "slideId": {
            "type": "number",
            "default": 1
        },				
        "outerParent": {
            "type": "object",
            "default": {}
        },
        "itemResponsiveMode": {
            "type": "string",
			"default": "xs"
        },
        "mediaId": {
            "type": "number"
        },
        "mediaUrl": {
            "type": "string",
            "default": ""
        },
        "xsmediaId": {
            "type": "number"
        },
        "xsimageUrl": {
            "type": "string",
            "default": ""
        },
        "mdmediaId": {
            "type": "number"
        },
        "mdimageUrl": {
            "type": "string",
            "default": ""
        },
        "mediaAlt": {
            "type": "string",
            "default": ""
        }
    },
    "parent": [ "tbblocks/slider" ],
	"textdomain": "tbblocks",
	"editorScript": "blocks-4.js"
}
