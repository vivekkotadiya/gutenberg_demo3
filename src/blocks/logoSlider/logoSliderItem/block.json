        "mediaId": {
            "type": "number"
        },
        "mediaUrl": {
            "type": "string",
            "default": ""
        },
        "xsmediaUrl": {
            "type": "string",
            "default": ""
        },
        "mdmediaUrl": {
            "type": "string",
            "default": ""
        },
        "mediaAlt": {
            "type": "string",
            "default": ""
        },
        "linkUrl": {
            "type": "string",
            "default": ""
        },
        "openNewTab": {
            "type": "boolean",
            "default": false
        },
        "linkTitle": {
            "type": "string",
            "default": ""
        }
    },
    "parent": [ "tbblocks/logoslider" ],
	"textdomain": "tbblocks",
	"editorScript": "blocks-4.js"
