{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "tbblocks/navigation",
	"version": "0.1.5",
	"title": "Navigation",
	"category": "theme",
	"description": "A gutenberg block to use classic menus.",
	"supports": {
		"html": false,
		"anchor": true
	},
	"attributes": {
		"anchor": {
			 "type": "string"
		},
		"menu": {
			"type": "number",
			"default": 0
		}
	},
	"textdomain": "tbblocks",
	"parent": [ "tbblocks/header" ],
	"editorScript": "blocks-2.js"
}
